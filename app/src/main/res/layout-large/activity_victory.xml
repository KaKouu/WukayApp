<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".HomeActivity"
        android:background="#6b9b33"
        android:id="@+id/layoutVictory">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/background_victory"
            android:id="@+id/decor"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="302dp"
            android:layout_height="316dp" app:srcCompat="@drawable/skin_lapinou"
            android:id="@+id/imageLapinou"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="164dp"
            android:layout_marginTop="356dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Gagné"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mainTitle2" android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/imageLapinou" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:textSize="50sp" android:fontFamily="@font/grobold"
            app:layout_constraintVertical_bias="0.0" android:textColor="@color/colorPrimaryDark"
            app:layout_constraintHorizontal_bias="0.516"/>
    <TextView
            android:text="Gagné"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mainTitle" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageLapinou" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:textSize="50sp" android:fontFamily="@font/grobold"
            app:layout_constraintVertical_bias="0.0" android:textColor="@android:color/holo_orange_light"/>
    <ImageView
            android:layout_width="443dp"
            android:layout_height="439dp" app:srcCompat="@drawable/feux_artifice"
            android:id="@+id/fireworks" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.134" app:layout_constraintHorizontal_bias="0.758"/>
    <ImageView
            android:layout_width="110dp"
            android:layout_height="107dp" app:srcCompat="@drawable/icon_circle_white"
            android:id="@+id/imageView24" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.833"
            app:layout_constraintVertical_bias="0.798"/>
    <ImageView
            android:layout_width="157dp"
            android:layout_height="146dp" app:srcCompat="@drawable/icon_circle" android:id="@+id/imageView16"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.869" app:layout_constraintVertical_bias="0.813"/>
    <ImageView
            android:layout_width="107dp"
            android:layout_height="122dp" app:srcCompat="@drawable/icon_carrot"
            android:id="@+id/imageView25"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.901"
            app:layout_constraintVertical_bias="0.837"/>
    <TextView
            android:text="+10"
            android:layout_width="104dp"
            android:layout_height="75dp"
            android:id="@+id/carrotTxt" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:textSize="40sp"
            app:layout_constraintHorizontal_bias="0.858" app:layout_constraintVertical_bias="0.806"
            android:textColor="@color/colorPrimaryDark" android:fontFamily="@font/grobold"/>
    <Button
            android:text="Rejouer"
            android:layout_width="224dp"
            android:layout_height="71dp"
            android:id="@+id/replayBt" android:fontFamily="@font/grobold"
            android:background="@drawable/rounded_button" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.949"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.966"
            android:textSize="30sp"/>
    <Button
            android:text="Retour"
            android:layout_width="221dp"
            android:layout_height="74dp"
            android:id="@+id/levelsBt" android:fontFamily="@font/grobold"
            android:background="@drawable/rounded_button" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.031"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.966"
            android:textSize="30sp"/>
    <ImageView
            android:layout_width="262dp"
            android:layout_height="324dp" app:srcCompat="@drawable/icon_rectangle"
            android:id="@+id/shapeLife2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.603" app:layout_constraintVertical_bias="0.461"/>
    <ImageView
            android:layout_width="93dp"
            android:layout_height="121dp" app:srcCompat="@drawable/icon_life"
            android:id="@+id/heart2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.38"
            app:layout_constraintVertical_bias="0.465"/>
    <TextView
            android:text="=0"
            android:layout_width="121dp"
            android:layout_height="132dp"
            android:id="@+id/numberOfLife2"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textSize="100sp" app:layout_constraintHorizontal_bias="0.708"
            app:layout_constraintVertical_bias="0.45"/>
</android.support.constraint.ConstraintLayout>